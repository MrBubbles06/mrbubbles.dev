{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
  },
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["warn", "single"],
      "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  }
}